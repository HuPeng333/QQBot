<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="priv.xds.mapper.GroupMapper">

    <select id="getAllRegisteredGroup" resultType="Group">
        SELECT * FROM t_group
    </select>

    <insert id="addGroup">
        INSERT INTO t_group(group_code) VALUES (#{group})
    </insert>

</mapper>